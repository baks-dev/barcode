{
  "name": "baks-dev/barcode",
  "description": "Модуль генерации и чтения штрихкодов",
  "homepage": "https://github.com/baks-dev/barcode",
  "keywords": [
    "symfony",
    "bundle",
    "library",
    "module",
    "plugin",
    "barcode",
    "reader",
    "Aztec",
    "Codabar",
    "Code39",
    "Code93",
    "Code128",
    "DataMatrix",
    "EAN-8",
    "EAN-13",
    "ITF",
    "PDF417",
    "QRCode",
    "UPC-A",
    "UPC-E",
    "baksdev"

  ],
  "license": "MIT",
  "type": "symfony-bundle",
  "authors": [
    {
      "name": "Baks Development",
      "email": "admin@baks.dev"
    }
  ],
  "require": {
    "php": ">=8.3",
    "ext-imagick": "*",
    "baks-dev/core": "^7.1"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "BaksDev\\Barcode\\": ""
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "symfony/flex": true
    }
  }
}